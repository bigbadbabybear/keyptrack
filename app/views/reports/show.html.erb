<p id="notice"><%= notice %></p>

<p>
  <b>User:</b>
  <%= @report.user.name %>
</p>

<p>
  <b>Game:</b>
  <%= @report.game.title rescue "" %>
</p>

<p>
  <b>Platform:</b>
  <%= @report.platform.os rescue "" %>
</p>

<p>
  <b>Report Key:</b>
  <%= @report.report_key %>
</p>

<p>
  <b>Status:</b>
  <%= @report.status.status rescue "" %>
</p>

<p>
  <b>Resolution:</b>
  <%= @report.resolution.resolution rescue "" %>
</p>

<p>
  <b>Fix commit:</b>
  <%= @report.fix_commit %>
</p>

<p>
  <b>Fix confirm:</b>
  <%= @report.fix_confirm %>
</p>

<%= link_to 'Edit', edit_report_path(@report) %> |
<%= link_to 'Back', reports_path %> |
<%= link_to 'New Report', new_report_path %>
<hr />
<p>
<iframe src="http://192.168.1.13:8080/browse/<%= "#{@report.report_key}" %>" width="100%" height="400" frameborder="0"></iframe>
</p>
